<h1>Edit User</h1>

<%= form_for @user, html: { class: 'ui form' } do |f| %>
  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <%= f.submit "Submit", class: 'ui green button' %>
<% end%>
